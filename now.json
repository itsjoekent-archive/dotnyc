{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "www" } }
  ],
  "build": {
    "env": {
      "REACT_APP_CONTENTFUL_SPACE_ID": "@contentful-space-id",
      "REACT_APP_CONTENTFUL_ACCESS_TOKEN": "@contentful-access-token"
    }
  },
  "routes": [
    { "src": "/(.*)", "headers": {"cache-control": "s-maxage=0"}, "dest": "/$1" }
  ]
}
